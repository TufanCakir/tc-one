{
  "expo": {
    "name": "TC One",
    "slug": "tc-one",
    "version": "1.0.9",
    "orientation": "portrait",
    "icon": "./assets/tc-one-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/tc-one-icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/tc-one-icon.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/tc-one-icon.png"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "enabled": true
    },
    "plugins": [
      "expo-video",
      "expo-secure-store",
      "expo-localization",
      "expo-mail-composer",
      "expo-web-browser"
    ]
  }
}
